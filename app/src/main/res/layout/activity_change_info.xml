<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/register_base"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/bg_white"
    tools:context=".HomeActivity">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/bubbleborder"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/account_edit_lbl1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="30dp"
            android:layout_marginTop="60dp"
            android:fontFamily="@font/lotte_happy"
            android:text="프로필 정보"
            android:textColor="@color/fr_black"
            android:textSize="28sp"
            android:typeface="sans"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/account_edit_ID"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:fontFamily="@font/lotte_happy"
            android:text="아이디"
            android:textColor="@color/fr_black"
            android:textSize="18sp"
            app:layout_constraintStart_toStartOf="@+id/account_edit_lbl1"
            app:layout_constraintTop_toBottomOf="@+id/account_edit_lbl1" />

        <TextView
            android:id="@+id/account_edit_email"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:fontFamily="@font/lotte_happy"
            android:text="이메일"
            android:textColor="@color/fr_black"
            android:textSize="18sp"
            app:layout_constraintStart_toStartOf="@+id/user_ID"
            app:layout_constraintTop_toBottomOf="@+id/user_ID" />

        <TextView
            android:id="@+id/account_edit_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="10dp"
            android:fontFamily="@font/lotte_happy"
            android:text="이름"
            android:textColor="@color/skyblue"
            android:textSize="16sp"
            app:layout_constraintBottom_toTopOf="@+id/user_input_name"
            app:layout_constraintStart_toStartOf="@+id/user_input_name" />

        <View
            android:id="@+id/view6"
            android:layout_width="0dp"
            android:layout_height="1dp"
            android:background="@color/fr_black"
            app:layout_constraintBottom_toTopOf="@+id/account_edit_name"
            app:layout_constraintEnd_toEndOf="@+id/user_input_name"
            app:layout_constraintStart_toStartOf="@+id/user_input_name"
            app:layout_constraintTop_toBottomOf="@+id/user_email" />

        <TextView
            android:id="@+id/user_ID"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:fontFamily="@font/lotte_dream"
            android:hint="현재 아이디 출력"
            android:inputType="textPersonName"
            android:text="현재 아이디"
            android:textColor="@color/gray"
            android:textColorHint="@color/bg_softgray"
            android:textSize="18sp"
            app:layout_constraintStart_toStartOf="@+id/account_edit_ID"
            app:layout_constraintTop_toBottomOf="@+id/account_edit_ID" />

        <TextView
            android:id="@+id/user_email"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:fontFamily="@font/lotte_dream"
            android:hint="이메일 출력"
            android:inputType="textPersonName"
            android:text="현재 이메일"
            android:textColor="@color/gray"
            android:textColorHint="@color/bg_softgray"
            android:textSize="18sp"
            app:layout_constraintStart_toStartOf="@+id/account_edit_email"
            app:layout_constraintTop_toBottomOf="@+id/account_edit_email" />


        <EditText
            android:id="@+id/user_input_name"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="30dp"
            android:layout_marginEnd="30dp"
            android:layout_marginBottom="20dp"
            android:backgroundTint="@color/skyblue"
            android:drawablePadding="10dp"
            android:ems="10"
            android:fontFamily="@font/lotte_dream"
            android:hint="이름출력"
            android:maxLength="16"
            android:paddingLeft="10dp"
            android:paddingTop="20dp"
            android:paddingBottom="20dp"
            android:textColor="@color/fr_black"
            android:textColorHint="@color/fr_darkGray"
            android:textSize="16sp"
            app:layout_constraintBottom_toTopOf="@+id/account_edit_phoneNumber"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/account_name_warn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:fontFamily="@font/elice"
            android:text="*"
            android:textColor="@color/fr_red"
            android:textSize="16sp"
            app:layout_constraintStart_toEndOf="@+id/account_edit_name"
            app:layout_constraintTop_toTopOf="@+id/account_edit_name" />

        <TextView
            android:id="@+id/account_pw_warn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/elice"
            android:text="*"
            android:textColor="@color/fr_red"
            android:textSize="16sp"
            app:layout_constraintStart_toEndOf="@+id/account_edit_pw"
            app:layout_constraintTop_toTopOf="@+id/account_edit_pw" />

        <TextView
            android:id="@+id/account_phoneNumber_warn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:fontFamily="@font/elice"
            android:text="*"
            android:textColor="@color/fr_red"
            android:textSize="16sp"
            app:layout_constraintStart_toEndOf="@+id/account_edit_phoneNumber"
            app:layout_constraintTop_toTopOf="@+id/account_edit_phoneNumber" />

        <EditText
            android:id="@+id/user_input_phoneNumber"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="30dp"
            android:layout_marginEnd="30dp"
            android:layout_marginBottom="20dp"
            android:backgroundTint="@color/skyblue"
            android:drawablePadding="10dp"
            android:ems="10"
            android:fontFamily="@font/lotte_dream"
            android:hint="전화번호 출력"
            android:maxLength="16"
            android:paddingLeft="10dp"
            android:paddingTop="20dp"
            android:paddingBottom="20dp"
            android:textColor="@color/fr_black"
            android:textColorHint="@color/fr_darkGray"
            android:textSize="16sp"
            app:layout_constraintBottom_toTopOf="@+id/account_edit_pw"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/account_edit_phoneNumber"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="10dp"
            android:fontFamily="@font/lotte_happy"
            android:text="핸드폰 번호"
            android:textColor="@color/skyblue"
            android:textSize="16sp"
            app:layout_constraintBottom_toTopOf="@+id/user_input_phoneNumber"
            app:layout_constraintStart_toStartOf="@+id/user_input_phoneNumber" />

        <TextView
            android:id="@+id/account_edit_pw"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="10dp"
            android:fontFamily="@font/lotte_happy"
            android:text="비밀번호"
            android:textColor="@color/skyblue"
            android:textSize="16sp"
            app:layout_constraintBottom_toTopOf="@+id/user_input_pw"
            app:layout_constraintStart_toStartOf="@+id/user_input_pw" />

        <EditText
            android:id="@+id/user_input_pw"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="30dp"
            android:layout_marginEnd="30dp"
            android:layout_marginBottom="10dp"
            android:backgroundTint="@color/skyblue"
            android:drawablePadding="10dp"
            android:ems="10"
            android:fontFamily="@font/lotte_dream"
            android:hint="비밀번호 출력"
            android:maxLength="16"
            android:paddingLeft="10dp"
            android:paddingTop="20dp"
            android:paddingBottom="20dp"
            android:textColor="@color/fr_black"
            android:textColorHint="@color/fr_darkGray"
            android:textSize="16sp"
            app:layout_constraintBottom_toTopOf="@+id/user_input_new_pwcheck"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />


        <Button
            android:id="@+id/account_btn_next"
            android:layout_width="0dp"
            android:layout_height="60dp"
            android:background="@drawable/ripple_button_skyblue"
            android:fontFamily="@font/lotte_dream"
            android:text="변경 완료"
            android:textColor="@color/white"
            android:textSize="18sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent" />

        <EditText
            android:id="@+id/user_input_new_pwcheck"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="30dp"
            android:layout_marginEnd="30dp"
            android:layout_marginBottom="30dp"
            android:backgroundTint="@color/skyblue"
            android:drawablePadding="10dp"
            android:ems="10"
            android:fontFamily="@font/lotte_dream"
            android:hint="비밀번호 재입력"
            android:inputType="number"
            android:maxLength="16"
            android:paddingLeft="10dp"
            android:paddingTop="20dp"
            android:paddingBottom="20dp"
            android:textColor="@color/fr_black"
            android:textColorHint="@color/fr_darkGray"
            android:textSize="16sp"
            app:layout_constraintBottom_toTopOf="@+id/account_btn_next"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>